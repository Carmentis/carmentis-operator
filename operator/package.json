{
  "name": "operator",
  "version": "1.2.0",
  "description": "Operator server",
  "author": "",
  "private": true,
  "license": "Apache-2.0",
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "migrate": "npm run migration:run",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "gen:config:example": "ts-node scripts/generate-example.ts",
    "gen:config:md": "ts-node scripts/generate-config-doc.ts",
    "docker": "bash scripts/docker-build-and-publish.sh",
    "typeorm": "typeorm-ts-node-commonjs -d src/database/DataSource.ts",
    "migration:generate": "npm run build && npm run typeorm -- migration:generate",
    "migration:run": "npm run typeorm -- migration:run",
    "migration:revert": "npm run typeorm -- migration:revert",
    "migration:show": "npm run typeorm -- migration:show"
  },
  "dependencies": {
    "@apollo/server": "^5.0.0",
    "@as-integrations/express5": "^1.1.2",
    "@automapper/classes": "^8.8.1",
    "@automapper/core": "^8.8.1",
    "@cmts-dev/carmentis-sdk": "^1.16",
    "@iarna/toml": "^2.2.5",
    "@nestjs/apollo": "^13.0.4",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/common": "^11.0.13",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/graphql": "^13.0.4",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/platform-socket.io": "^11.1.6",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/swagger": "^11.1.1",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/typeorm": "^11.0.0",
    "@nestjs/websockets": "^11.1.6",
    "@nestjsx/crud": "^5.0.0-alpha.3",
    "@noble/ciphers": "^1.3.0",
    "@noble/secp256k1": "^2.2.3",
    "@semantic-release/npm": "13.1.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "dotenv": "^16.4.7",
    "get-port": "^7.1.0",
    "graphql": "^16.10.0",
    "mysql2": "^3.14.5",
    "pg": "^8.13.1",
    "read-pkg": "^9.0.1",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.1",
    "sqlite3": "^5.1.7",
    "typeorm": "^0.3.21",
    "webpack": "^5.102.0",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@nestjs/testing": "^11.1.6",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/release-notes-generator": "^14.0.3",
    "@testcontainers/postgresql": "^11.6.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/read-pkg": "^4.0.0",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.5.0",
    "jest-util": "^30.2.0",
    "semantic-release": "^25.0.2",
    "semantic-release-monorepo": "^8.0.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "testcontainers": "^11.6.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "directories": {
    "test": "test"
  }
}
